@using PilotSmithApp.UserInterface.App_Start


<div class="col-md-12 headSeperator"> Approval History </div>
    <div class="row tableContainer">
        <div class="col-md-12">
            <table id="tblApprovalHistory" class="table table-striped table-bordered table-hover customgrd" cellspacing="0" width="80%">
                <thead>
                    <tr class="text-center TableHead">
                        <th>Approver</th>
                        <th>Level</th>
                        <th>Date</th>
                        <th>Remarks</th>
                        <th>Status</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>

@section Scripts {
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/bundles/UserJs/DocumentApproval/ApprovalHistory")
}